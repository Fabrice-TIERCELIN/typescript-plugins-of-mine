{
  "name": "ts-simple-ast-extra",
  "version": "0.0.19",
  "description": "extra utilities for ts-simple-ast ",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine",
    "test-debug": "ts-node node_modules/jasmine/bin/jasmine --invert --inspect-brk",
    "build": "tsc",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "prettier": " npx prettier  --write --arrow-parens avoid --parser typescript --use-tabs false --no-semi true --single-quote true --tab-width 2 --trailing-comma none --end-of-line lf --prose-wrap preserve --print-width 120 --jsx-bracket-same-line true \"src/**/*.ts*\"",
    "doc": "typedoc --mode modules --ignoreCompilerErrors --out ../docs/ts-simple-ast-extra src/index.ts ",
    "all": "npm run clean && npm run prettier && npm run build && npm run test && npm run doc"
  },
  "keywords": [
    "TypeScript",
    "TypeScript Language Service",
    "plugin"
  ],
  "author": "Sebastian Gurin",
  "license": "MIT",
  "dependencies": {
    "misc-utils-of-mine-generic": "0.0.12",
    "prettier": "^1.16.4",
    "ts-morph": "^1.3.3"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.16",
    "@types/node": "^11.13.0",
    "jasmine": "^3.4.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.4.2"
  },
  "repository": "https://github.com/cancerberoSgx/typescript-plugin-of-mine/tree/master/ts-simple-ast-extra"
}
