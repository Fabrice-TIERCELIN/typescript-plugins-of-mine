{
  "name": "ts-simple-ast-extra",
  "version": "0.2.1",
  "description": "high level TypeScript Compiler API and refactor tools based on ts-morph (ex ts-simple-ast) librart",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "test": "ts-node -T node_modules/jasmine/bin/jasmine --stop-on-failure=true --fail-fast=true",
    "test-watch": "npx watch \"npm test\" src spec",
    "test-debug": "node --inspect-brk node_modules/ts-node/dist/bin node_modules/jasmine/bin/jasmine.js",
    "build": "tsc",
    "coverage": "npx nyc --nycrc-path nyc.config.js npm test",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "prettier": " npx prettier  --write --arrow-parens avoid --parser typescript --use-tabs false --no-semi true --single-quote true --tab-width 2 --trailing-comma none --end-of-line lf --prose-wrap preserve --print-width 120 --jsx-bracket-same-line true \"src/**/*.ts*\" \"spec/**/*.ts*\"",
    "doc": "typedoc --mode modules --ignoreCompilerErrors --out ../docs/ts-simple-ast-extra src/index.ts ",
    "all": "npm run clean && npm run prettier && npm run build && npm run test && npm run doc",
    "all-publish": "npm run all && npm version patch && npm publish"
  },
  "keywords": [
    "TypeScript",
    "TypeScript Language Service",
    "plugin"
  ],
  "author": "Sebastian Gurin",
  "license": "MIT",
  "dependencies": {
    "misc-utils-of-mine-generic": "0.2.18",
    "ts-morph": "^3.1.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/diff": "^4.0.2",
    "@types/jasmine": "^3.4.0",
    "@types/node": "^12.7.2",
    "@types/shelljs": "^0.8.5",
    "colors": "^1.3.3",
    "coveralls": "^3.0.6",
    "diff": "^4.0.1",
    "jasmine": "^3.4.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "shelljs": "^0.8.3",
    "ts-node": "^8.3.0",
    "typedoc": "^0.15.0",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "typescript": "^3.5.3"
  },
  "repository": "https://github.com/cancerberoSgx/typescript-plugin-of-mine/tree/master/ts-simple-ast-extra"
}
