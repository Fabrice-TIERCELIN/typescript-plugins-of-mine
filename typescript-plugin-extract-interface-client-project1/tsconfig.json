{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "lib": [
      "es2018"
    ],
    "outDir": "./dist",
    "strict": false,
    "strictNullChecks": false, 
    "esModuleInterop": true,
    "removeComments": false,
    "noImplicitAny": false,
    "plugins": [
      {
        "name": "typescript-plugin-ast-inspector"
      },
      {
        "name": "typescript-plugin-postfix"
      },
    ]
  }
}

























