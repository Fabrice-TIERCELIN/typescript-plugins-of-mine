{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "lib": [
      "es2018"
    ],
    "outDir": "./dist",
    "strict": true,
    "strictNullChecks": false, 
    "esModuleInterop": true,
    "removeComments": false,
    "noImplicitAny": true,
    "plugins": [
      {
        "name": "typescript-plugin-extract-interface"
      },
      {
        "name": "typescript-plugin-subclasses-of"
      },
      {
        "name": "typescript-plugin-ast-inspector"
      },
      {
        "name": "typescript-plugin-add-type"
      },
      {
        "name": "typescript-plugin-move-file"
      },
      {
        "name": "typescript-plugin-move-declaration"
      },
      // {
      //   "name": "typescript-api-tests"
      // }
      
      
    ]
  }
}