{
  "name": "typescript-plugin-function-signature-refactors",
  "version": "0.0.13",
  "description": "TypeScript Language Service Plugin plugin with several refactors related to function-like signature. ",
  "main": "dist/src/index.js",
  "typings": "dist/src/typings.d.ts",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine",
    "test-js": "node node_modules/jasmine/bin/jasmine --config=./spec/support/jasmine-js.json",
    "test-coverage": "nyc --clean --reporter=lcov -e .ts -x \"*Spec.ts\" ts-node node_modules/jasmine/bin/jasmine --config=./spec/support/jasmine.json",
    "doc": "exit 0",
    "watch": "tsc --watch",
    "build": "tsc",
    "prepare": "npm run build",
    "clean": "rm -rf node_modules package-lock.json dist",
    "all": "npm run clean && npm i && npm run build && npm test"
  },
  "keywords": [
    "TypeScript",
    "TypeScript plugin",
    "TypeScript refactor",
    "TypeScript Language Service",
    "plugin"
  ],
  "author": "Sebastian Gurin",
  "license": "MIT",
  "dependencies": {
    "hrtime-now": "^1.0.5",
    "input-ui-ipc-provider": "file:../input-ui-ipc-provider",
    "ts-simple-ast": "^18.0.0",
    "ts-simple-ast-extra": "file:../ts-simple-ast-extra",
    "typescript": "^3.1.4",
    "typescript-ast-util": "file:../typescript-ast-util",
    "typescript-plugin-util": "file:../typescript-plugin-util",
    "typescript-plugins-text-based-user-interaction": "file:../typescript-plugins-text-based-user-interaction"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "@types/node": "^10.12.1",
    "@types/shelljs": "^0.8.0",
    "coveralls": "^3.0.2",
    "jasmine": "^3.3.0",
    "nyc": "^13.1.0",
    "shelljs": "^0.8.2",
    "ts-node": "^7.0.1"
  },
  "repository": "https://github.com/cancerberoSgx/typescript-plugins-of-mine/tree/master/typescript-plugin-function-signature-refactors"
}
